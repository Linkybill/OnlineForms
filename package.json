{
  "name": "NOVA-SharePoint-Forms",
  "version": "8.0.170",
  "private": true,
  "main": "lib/index.js",
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "preinstall": "npx npm-force-resolutions@0.0.3",
    "init": "npm i run-sequence --save-dev && npm i sppurge --save-dev && npm i gulp-spsave --save-dev && npm i sp-request --save-dev && npm i readline-sync --save-dev && npm i npm-force-resolutions@0.0.3 --save-dev && npm i gulp-spsync-creds@2.3.8 --save-dev && npm i terser-webpack-plugin-legacy@1.2.3 --save-dev && npm i prettier@2.8.8 --save-dev && npm i spfx-fast-serve && npm i url-loader@4.1.1 --save-dev && npm i gulp-json-editor@2.6.0 --save-dev",
    "build": "gulp bundle",
    "clean": "gulp clean",
    "dev": "set NODE_NO_HTTP2=1 && gulp serve --nobrowser",
    "test": "gulp test",
    "bundle-ship": "gulp bundle --ship || echo \"gulp bundle errored\"",
    "package-solution-ship": "gulp package-solution --ship || echo \"gulp package-solution errored\"",
    "deploy": "gulp select-deployment-env && gulp sync-version && npm install && gulp prep-for-deploy && npm run bundle-ship && npm run package-solution-ship && gulp deploy-assets && gulp add-deploy-sppkg",
    "auto-deploy": "gulp select-deployment-env-date && npm install && gulp prep-for-deploy && npm run bundle-ship && npm run package-solution-ship && gulp schedule-deployment",
    "script-deploy": "gulp prep-for-pipeline-deploy && gulp sync-version && npm run bundle-ship && npm run package-solution-ship && gulp deploy-assets && gulp add-deploy-sppkg",
    "format": "prettier --loglevel warn --write \"**/*.{tsx,ts}\"",
    "serve": "gulp bundle --custom-serve --max_old_space_size=4096 && fast-serve"
  },
  "dependencies": {
    "@fluentui/react": "^8.111.2",
    "@fluentui/react-hooks": "^8.8.4",
    "@microsoft/decorators": "~1.4.0",
    "@microsoft/sp-core-library": "~1.4.0",
    "@microsoft/sp-dialog": "~1.4.0",
    "@microsoft/sp-listview-extensibility": "~1.4.0",
    "@microsoft/sp-lodash-subset": "~1.4.0",
    "@microsoft/sp-office-ui-fabric-core": "~1.4.0",
    "@microsoft/sp-webpart-base": "~1.4.0",
    "@pnp/sp": "^2.0.9",
    "@pnp/spfx-controls-react": "^1.21.3",
    "@types/es6-promise": "0.0.33",
    "@types/json-logic-js": "^2.0.2",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/webpack-env": "1.13.1",
    "axios": "^0.28.0",
    "currency-formatter": "^1.5.9",
    "dayjs": "^1.11.10",
    "json-logic-js": "^2.0.2",
    "lodash": "^4.17.21",
    "loglevel": "^1.8.1",
    "loglevel-plugin-prefix": "^0.8.4",
    "mermaid": "^8.14.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-drag-and-drop": "^3.0.0",
    "react-quill": "^2.0.0",
    "react-render-html": "^0.6.0",
    "string-template": "^1.0.0"
  },
  "resolutions": {
    "graceful-fs": "4.2.2"
  },
  "devDependencies": {
    "@microsoft/sp-build-web": "~1.4.1",
    "@microsoft/sp-module-interfaces": "~1.4.1",
    "@microsoft/sp-webpart-workbench": "~1.4.1",
    "@types/chai": "3.4.34",
    "@types/mocha": "2.2.38",
    "@types/sharepoint": "^2016.1.10",
    "ajv": "~5.2.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "gulp": "^3.9.1",
    "gulp-json-editor": "^2.6.0",
    "gulp-spsave": "^4.0.0",
    "gulp-spsync-creds": "^2.3.8",
    "jszip": "^3.10.1",
    "npm-version": "^1.1.0",
    "prettier": "^2.8.8",
    "readline-sync": "^1.4.10",
    "run-sequence": "^2.2.1",
    "sp-request": "^3.0.0",
    "spfx-fast-serve-helpers": "~1.4.0",
    "sppurge": "^3.0.1",
    "terser-webpack-plugin-legacy": "1.2.3",
    "typescript": "4.1.5"
  }
}
